{
  "manifest_version": 2,
  "name": "mysources",
  "version": "1",
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.dndbeyond.com/*"
      ],
      "js": [
        "mysources.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/index.html"
  },
  "browser_action": {
    "default_popup": "options/index.html",
    "default_title": "mysources"
  }
}